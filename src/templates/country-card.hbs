<div>
   <h1>{{name}}</h1>
   <div>
       <h2>Capital: {{capital}}</h2>
       <h2>Population: {{population}}</h2>
       <h2>Languages: </h2>
       <ul>
           {{#each languages}}
           <li>{{name}}</li>
           {{/each}}
       </ul>
       <img src="{{flags.png}}" alt="{{name}}">
   </div>
</div>